{
  "$schema": "http://json-schema.org/draft-07/schema",
  "title": "tournament picks and bans",
  "type": "array",
  "default": [],
  "items":{
    "type": "object",
    "required": ["id", "color"],
    "additionalProperties": false,
    "properties": {
      "id": {
        "type": "integer"
      },
      "color": {
        "type": "string",
        "enum": ["red", "blue"]
      }
    }
  }
}
